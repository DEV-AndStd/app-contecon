{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/equipos",
      "dest": "app.js"
    },
    {
      "src": "/repuestos",
      "dest": "app.js"
    },
    {
      "src": "/usuarios",
      "dest": "app.js"
    },
    {
      "src": "/estado_equipos",
      "dest": "app.js"
    },
    {
      "src": "/",
      "dest": "app.js"
    }
  ]
}